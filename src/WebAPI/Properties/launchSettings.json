{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  // NOTE: These settings are only used when running this project directly (dotnet run --project src/WebAPI)
  // When running through Aspire (dotnet run --project src/Aspire/AppHost), Aspire manages ports and URLs automatically
  "profiles": {
    "HeadStart.WebAPI": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "launchUrl": "scalar/headstart-api-v1",
      "applicationUrl": "https://localhost:7061;http://localhost:5061",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "hotReloadEnabled": true
    }
  }
}
