{
  "ReverseProxy": {
    "Routes": {
      "api-v1": {
        "ClusterId": "api",
        "AuthorizationPolicy": "CookieAuthenticationPolicy",
        "Match": {
          "Path": "/v1/api/{**remainder}"
        }
      },
      "api": {
        "ClusterId": "api",
        "AuthorizationPolicy": "CookieAuthenticationPolicy",
        "Match": {
          "Path": "/api/{**remainder}"
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "api/destination1": {
            "Address": "https://localhost:7061/"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
