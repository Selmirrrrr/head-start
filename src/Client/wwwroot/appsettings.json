{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.BrowserConsole",
      "Serilog.Sinks.BrowserHttp",
      "Serilog.Exceptions",
      "HeadStart.SharedKernel"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "BrowserConsole"
      },
      {
        "Name": "BrowserHttp"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithInstanceId",
      "WithExceptionDetails"
    ]
  }
}
